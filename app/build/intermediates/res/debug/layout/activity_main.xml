<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="#000000"
    android:paddingBottom="@dimen/activity_vertical_margin" tools:context=".MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="00:00:000"
        android:textColor="#FF0000"
        android:textSize="128sp"
        android:id="@+id/timeKeeper"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:paddingBottom="30dip" />

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/timeKeeper"
        android:layout_centerHorizontal="true"
        android:baselineAligned="false"
        android:layout_alignParentRight="false"
        android:id="@+id/linearLayout">

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="START"
            android:onClick="startPomodoroTimer"
            android:id="@+id/startButton"
            android:minHeight="120dip"
            android:minWidth="150dip"
            android:textSize="32dip"
            android:layout_marginRight="40dip" />

        <Button
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="PAUZE"
            android:onClick="setPause"
            android:id="@+id/pauseButton"
            android:minWidth="150dip"
            android:minHeight="120dip"
            android:textSize="32dip" />

        <Button
            style="?android:attr/buttonStyleSmall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="STOP"
            android:id="@+id/stopButton"
            android:minWidth="150dip"
            android:minHeight="120dip"
            android:textSize="32dip"
            android:layout_marginLeft="40dip" />
    </LinearLayout>

    <LinearLayout
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/linearLayout"
        android:layout_centerHorizontal="true"
        android:paddingTop="50dip">

        <TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            >

            <TableRow>
                <TextView
                    android:text="Type"
                    android:textColor="#FF0000"
                    android:padding="3dip"
                    android:layout_weight="1"
                    android:layout_marginLeft="10dip" />
                <TextView
                    android:text="Length"
                    android:textColor="#FF0000"
                    android:padding="3dip"
                    android:layout_weight="1"/>
                <TextView
                    android:text="Started"
                    android:textColor="#FF0000"
                    android:padding="3dip"
                    android:layout_weight="1"/>
                <TextView
                    android:text="Ended"
                    android:textColor="#FF0000"
                    android:padding="3dip"
                    android:layout_weight="1"/>
            </TableRow>

            <View
                android:layout_height="2dip"
                android:background="#FF909090" />

            <TableRow android:background="#ff262626">
                <TextView
                    android:text="Single Pomodoro"
                    android:textColor="#FFFFFF"
                    android:padding="3dip"
                    android:layout_weight="1"
                    android:layout_marginLeft="10dip" />
                <TextView
                    android:text="00:25:00"
                    android:textColor="#FFFFFF"
                    android:padding="3dip"
                    android:layout_weight="1"/>
                <TextView
                    android:text="--:--:--"
                    android:id="@+id/started1"
                    android:textColor="#FFFFFF"
                    android:padding="3dip"
                    android:layout_weight="1"
                    />
                <TextView
                    android:text="--:--:--"
                    android:textColor="#FFFFFF"
                    android:id="@+id/ended1"
                    android:padding="3dip"
                    android:layout_weight="1"/>
            </TableRow>


            <TableRow>
                <TextView
                    android:text="Short Break"
                    android:padding="3dip"
                    android:textColor="#FFFFFF"
                    android:layout_weight="1"
                    android:layout_marginLeft="10dip" />
                <TextView
                    android:text="00:05:00"
                    android:textColor="#FFFFFF"
                    android:padding="3dip"
                    android:layout_weight="1"/>
                <TextView
                    android:text="--:--:--"
                    android:textColor="#FFFFFF"
                    android:id="@+id/breakstarted1"
                    android:padding="3dip"
                    android:layout_weight="1"/>
                <TextView
                    android:text="--:--:--"
                    android:textColor="#FFFFFF"
                    android:id="@+id/breakended1"
                    android:padding="3dip"
                    android:layout_weight="1"/>
            </TableRow>

            <TableRow android:background="#ff262626">
                <TextView
                    android:text="Single Pomodoro"
